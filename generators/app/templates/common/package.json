{
  "private": true,
  "name": "<%= packageAppName %>",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "deploy": "echo 'To setup deployment run: <%= packageManagerRun %> setup-deploy'",
    "setup-deploy": "app-app --deploy && <%= packageManagerRun %> deploy"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "arrowParens": "avoid"
  }
}
