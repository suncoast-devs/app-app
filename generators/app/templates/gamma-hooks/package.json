{
  "private": true,
  "name": "<%= packageAppName %>",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "deploy": "echo 'To setup deployment run: <%= packageManagerRun %> setup-deploy'",
    "setup-deploy": "app-app --deploy && <%= packageManagerRun %> deploy"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "useTabs": false
  }
}
